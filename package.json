	"scripts": {
		"preinstall": "node tools/nodeVersionCheck.js",
		"start-message": "babel-node tools/startMessage.js",
		"start": "set PORT=3002 && node start ./dist/main.js",
		"dev-start": "set PORT=3002 && npm run build:langs && npm-run-all --parallel open:src lint:watch",
		"open:src": "babel-node tools/srcServer.js",
		"open:dist": "babel-node tools/distServer.js",
		"lint": "esw \"./src/**/*.{js,jsx}\" --color",
		"lint:watch": "npm run lint -- --watch",
		"clean-dist": "npm run remove-dist && mkdir dist",
		"remove-dist": "rimraf ./dist",
		"build:langs": "babel-node tools/mergeMessages.js",
		"zip-dist": "node tools/archive.js",
		"prebuild": "npm run clean-dist",
		"build": "set PORT=3002 && npm run clean-dist && npm run build:langs && babel-node tools/build.js",
		"build-release": "npm run clean-dist && npm run build:langs && babel-node tools/build.js",
		"release": "npm run build-release && npm run zip-dist",
		"analyze-bundle": "babel-node ./tools/analyzeBundle.js",
		"format": "prettier --write \"./src/**/*.{js,jsx}\"",
		"cy:test": "cypress run",
		"cy:open": "cypress open",
		"test:ci": "start-server-and-test open:src 3000 cy:test",
		"start:dev": "webpack-dev-server",
		"heroku-prebuild": "",
		"heroku-postbuild": "babel-node tools/build.js"
	},
